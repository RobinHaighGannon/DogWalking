<h1>Welcome to the dog walking database!</h1>
<%= form_with(url: welcome_index_path, method: :get, local: true) do |form| %>
<p>
<%= form.label :username %>
<%= form.text_field :username %>
</p><p>
<%= form.label :password %>
<%= form.password_field :password %>
</p>
<%= form.submit "Login" %>
<%end%>
